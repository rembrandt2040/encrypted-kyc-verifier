{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "app.js", "use": "@vercel/static" },
    { "src": "tfhe-wasm/*.wasm", "use": "@vercel/static" }
  ],
  "headers": [
    {
      "source": "/tfhe-wasm/(.*).wasm",
      "headers": [
        { "key": "Content-Type", "value": "application/wasm" },
        { "key": "Access-Control-Allow-Origin", "value": "*" }
      ]
    }
  ]
}
